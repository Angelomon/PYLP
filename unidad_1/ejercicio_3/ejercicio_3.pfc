program Ejercicio3;


function esPrimo(n: integer): boolean;
var
    i: integer;
	cant: integer;
begin
	esPrimo:= false;
	cant:=0;
	for i:=1 to n do
	begin
		if (n mod i = 0) then
		begin
			cant:= cant + 1;
		end;
	end;
	if(cant = 2) then
		esPrimo:= true;
	else
		esPrimo:= false;
	end;
end;

